    "eslint-plugin-react-hooks": "4.6.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@nestjs/cli": "9.1.3",
    "@nestjs/common": "9.1.2",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.1.2",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.2",
